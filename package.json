{
  "name": "vite-plugin-mdx",
  "version": "3.3.2",
  "scripts": {
    "// User Scripts": "",
    "dev": "yarn tsc:watch",
    "build": "yarn install && yarn tsc:once",
    "// TypeScript": "",
    "tsc:watch": "tsc --build --watch",
    "tsc:once": "tsc --build",
    "// Release": "",
    "release": "yarn clean && yarn install && yarn tsc:once && npm publish",
    "// Remove caches": "",
    "clean": "git clean -Xdff"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@mdx-js/mdx": "*",
    "vite": "*"
  },
  "dependencies": {
    "esbuild": "^0.8.31",
    "find-dependency": "^1.3.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.0.0-next.9",
    "@types/es-module-lexer": "^0.3.0",
    "@types/node": "^14.14.22",
    "typescript": "^4.1.3",
    "unified": "^9.2.1",
    "vite": "^2.0.4"
  },
  "description": "Vite plugin for MDX",
  "license": "MIT",
  "repository": "https://github.com/brillout/vite-plugin-mdx",
  "keywords": [
    "vite",
    "mdx"
  ],
  "publishConfig": {
    "access": "public"
  }
}
